{
	"name": "debug",
	"model": "n2s-unet",
	"model_params": {
		"activation": "leakyrelu",
		"residual": false
	},
	"fit": {
		"learn_rate": 0.01,
		"batch_size": 8,
		"epochs": 20,
		"mask_grid_size": 4
	},
	"dataset": {
		"sharp": true,
		"target_size": [18, 16],
		"loss_shape": "full",
		"patches_per_image": 1,
		"crop": true,
		"drop_background": true,
		"complete_background_noise": true
	}
}